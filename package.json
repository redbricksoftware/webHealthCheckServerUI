{
  "name": "WebHealthCheckServerUI",
  "version": "0.1.4",
  "description": "Monitor and log the status of APIs.",
  "keywords": [
    "API",
    "Health Check",
    "APIs",
    "Monitoring"
  ],
  "author": "RedBrickSoftware",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redbricksoftware/webHealthCheckServerUI.git"
  },
  "bugs": {
    "url": "https://github.com/redbricksoftware/webHealthCheckServerUI/issues"
  },
  "homepage": "http://redbricksoftware.com",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "systemjs": "~0.20.0",
    "core-js": "~2.4.1",
    "rxjs": "~5.1.0",
    "zone.js": "^0.7.4",
    "bootstrap": "~3.3.7",
    "jquery": "~3.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.10",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "~4.4.2",
    "lodash": "^4.16.4",
    "protractor": "^5.0.0",
    "rimraf": "^2.5.4",
    "@types/node": "^7.0.0",
    "mocha": "~3.*",
    "@types/mocha": "*",
    "chai": "~3.*",
    "@types/chai": "*",
    "chai-http": "~3.*",
    "@types/chai-http": "*"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server --port 8080\"",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "testold": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "mocha --reporter spec"
  }
}
